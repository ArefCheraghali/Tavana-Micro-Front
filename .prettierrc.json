{
	"trailingComma": "all",
	"useTabs": true,
	"tabWidth": 2,
	"semi": true,
	"bracketSpacing": true,
	"singleQuote": false,
	"arrowParens": "avoid",
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"jsxBracketSameLine": false,
	"printWidth": 80,
	"proseWarp": "always",
	"quoteProps": "as-needed",
	"requirePragma": false
}
